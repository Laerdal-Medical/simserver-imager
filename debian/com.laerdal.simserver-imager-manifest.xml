<?xml version="1.0" encoding="UTF-8"?>
<!-- MIME type for Laerdal SimServer Imager manifest files.
     Extension and MIME type defined in src/config.h (MANIFEST_EXTENSION, MANIFEST_MIME_TYPE) -->
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/vnd.laerdal.imager-manifest+json">
    <comment>Laerdal SimServer Imager Manifest</comment>
    <glob pattern="*.laerdal-imager-manifest"/>
    <sub-class-of type="application/json"/>
    <icon name="laerdal-simserver-imager"/>
  </mime-type>
</mime-info>

